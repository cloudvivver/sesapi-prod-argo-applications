{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AirflowS3Access",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::saude-auditoria-login",
        "arn:aws:s3:::saude-auditoria-login/*"
      ]
    },
    {
      "Sid": "AirflowS3ListAllBuckets",
      "Effect": "Allow",
      "Action": [
        "s3:ListAllMyBuckets"
      ],
      "Resource": "*"
    }
  ]
}
